#define _SRC_P_EST_CONFIG_H 1

/* C compiler */
#define P4EST_CC @P4EST_CC@

/* C compiler flags */
#define P4EST_CFLAGS @P4EST_CFLAGS@

/* C preprocessor */
#define P4EST_CPP @P4EST_CPP@

/* C preprocessor flags */
#define P4EST_CPPFLAGS @P4EST_CPPFLAGS@

/* Defined if the 2D library is enabled */
#cmakedefine P4EST_ENABLE_BUILD_2D

/* Defined if the 3D library is enabled */
#cmakedefine P4EST_ENABLE_BUILD_3D

/* Defined if the hybrid 2D+1D p6est library enabled */
#cmakedefine P4EST_ENABLE_BUILD_P6EST

/* Defined if we are using debug build type (assertions and extra checks) */
#cmakedefine P4EST_ENABLE_DEBUG @P4EST_ENABLE_DEBUG@

/* Defined if we are using aligned malloc (optionally use --enabled-memalign=<bytes>) */
#cmakedefine P4EST_ENABLE_MEMALIGN

/* Defined if we are using MPI */
#cmakedefine P4EST_ENABLE_MPI

/* Defined if we can use MPI_COMM_TYPE_SHARED */
#cmakedefine P4EST_ENABLE_MPICOMMSHARED

/* Defined if we are using MPI I/O */
#cmakedefine P4EST_ENABLE_MPIIO

/* Defined if we are using MPI_Init_thread */
#cmakedefine P4EST_ENABLE_MPITHREAD

/* Defined if we can use MPI_Win_allocate_shared */
#cmakedefine P4EST_ENABLE_MPIWINSHARED


/* Undefine if: write vtk ascii file data */
#define P4EST_ENABLE_VTK_BINARY 1

/* Undefine if: disable zlib compression for vtk binary data */
#define P4EST_ENABLE_VTK_COMPRESSION 1

/* Define to a macro mangling the given C identifier (in lower and upper
   case), which must not contain underscores, for linking with Fortran. */
#define P4EST_F77_FUNC(name,NAME) name ## _

/* As F77_FUNC, but for C identifiers containing underscores. */
#define P4EST_F77_FUNC_(name,NAME) name ## _

/* Define to a macro mangling the given C identifier (in lower and upper
   case), which must not contain underscores, for linking with Fortran. */
#define P4EST_FC_FUNC(name,NAME) name ## _

/* As FC_FUNC, but for C identifiers containing underscores. */
#define P4EST_FC_FUNC_(name,NAME) name ## _


/* Defined if you have the <arpa/inet.h> header file. */
#cmakedefine P4EST_HAVE_ARPA_INET_H

/* Defined if you have the <dlfcn.h> header file. */
#cmakedefine P4EST_HAVE_DLFCN_H

/* Defined if you have the `fsync' function. */
#cmakedefine P4EST_HAVE_FSYNC

/* Defined if you have the <inttypes.h> header file. */
#cmakedefine P4EST_HAVE_INTTYPES_H

/* Have we found function pthread_create. */
#cmakedefine HAVE_LPTHREAD

/* Have we found function lua_createtable. */
#cmakedefine HAVE_LUA

/* Defined if you have the <memory.h> header file. */
#cmakedefine P4EST_HAVE_MEMORY_H

/* Defined if you have the <netinet/in.h> header file. */
#cmakedefine P4EST_HAVE_NETINET_IN_H

/* Defined if you have the <Winsock2.h> header file. */
#cmakedefine P4EST_HAVE_WINSOCK2_H

/* Defined if you have the `posix_memalign' function. */
#cmakedefine P4EST_HAVE_POSIX_MEMALIGN

/* Defined if you have the <stdint.h> header file. */
#cmakedefine P4EST_HAVE_STDINT_H

/* Defined if you have the <stdlib.h> header file. */
#cmakedefine P4EST_HAVE_STDLIB_H

/* Defined if you have the <strings.h> header file. */
#cmakedefine P4EST_HAVE_STRINGS_H

/* Defined if you have the <string.h> header file. */
#cmakedefine P4EST_HAVE_STRING_H

/* Defined if you have the <sys/stat.h> header file. */
#cmakedefine P4EST_HAVE_SYS_STAT_H

/* Defined if you have the <sys/types.h> header file. */
#cmakedefine P4EST_HAVE_SYS_TYPES_H

/* Defined if you have the <unistd.h> header file. */
#cmakedefine P4EST_HAVE_UNISTD_H

/* Have we found function adler32_combine. */
#cmakedefine P4EST_HAVE_ZLIB

/* Linker flags */
#define P4EST_LDFLAGS @P4EST_LDFLAGS@

/* Libraries */
#define P4EST_LIBS @P4EST_LIBS@


/* Name of package */
#define P4EST_PACKAGE "p4est"

/* Define to the address where bug reports for this package should be sent. */
#define P4EST_PACKAGE_BUGREPORT "info@p4est.org"

/* Define to the full name of this package. */
#define P4EST_PACKAGE_NAME "p4est"

/* Define to the full name and version of this package. */
#define P4EST_PACKAGE_STRING "p4est 2.8.0"

/* Define to the one symbol short name of this package. */
#define P4EST_PACKAGE_TARNAME "p4est"

/* Define to the home page for this package. */
#define P4EST_PACKAGE_URL ""

/* Define to the version of this package. */
#define P4EST_PACKAGE_VERSION "2.8.0"

/* Version number of package */
#define P4EST_VERSION "2.8.0"

/* Package major version */
#define P4EST_VERSION_MAJOR 2

/* Package minor version */
#define P4EST_VERSION_MINOR 8

/* Package point version */
#define P4EST_VERSION_POINT 0
